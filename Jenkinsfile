pipeline {
    agent any

    stages{
        stage("build") {
            steps { 
                echo 'this is the build stage'
                echo 'building safe havens'

            }
        }

        stage("test") {
            steps { 
                echo 'this is the test stage'
                echo 'print this'
                echo 'test, again 4:43'

            }
        }
        stage("deploy") {
            steps { 
                echo 'this is the deployment stage'
                echo 'deploy this again'
                echo 'another test'
                echo 'yet another test'

            }
        }
    }
}